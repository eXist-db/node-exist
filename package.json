    "url": "https://github.com/eXist-db/node-exist.git"
  },
  "maintainers": [
    {
      "name": "Juri Leino",
      "email": "npm@line-o.de"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/github": "^8.0.0",
    "semantic-release": "^18.0.0",
    "standard": "^16.0.0",
    "tape": "^5.0.1"
  },
  "dependencies": {
    "lodash.assign": "^4.0.2",
    "mime": "^2.3.1",
    "xmlrpc": "^1.3.1"
  },
  "version": "0.0.0-development"
