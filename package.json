{
  "name": "@existdb/node-exist",
  "description": "promise based interaction with eXist DB's XML-RPC API",
  "main": "index.js",
  "scripts": {
    "test": "standard && tape spec/tests/*.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "bin": {
    "exist-ls": "spec/examples/exist-ls",
    "exist-upload": "spec/examples/exist-upload",
    "exist-install": "spec/examples/exist-install"
  },
  "standard": {
    "ignore": [
      "exist-*"
    ]
  },
  "keywords": [
